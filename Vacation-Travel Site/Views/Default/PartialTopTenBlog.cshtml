@using Vacation_Travel_Site.Models.Classes
@model List<Blog>
<div class="padding">
    <h4>This Week's Top 10</h4>

    <ul class="list list-pad">
        @foreach (var item in Model)
        {
            <li class="list-bg"><span>@item.Id</span><a href="/Blog/Detail/@item.Id">@item.Title</a></li>
        }


    </ul>
    <a href="#" class="link1">See all hotels</a>
</div>